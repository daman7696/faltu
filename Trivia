@bot.command()
#@commands.has_role(accRole)
async def acc(ctx,gameName=None,time=None,accuracy=None,winAmt=None,*,status=None):
    embed=discord.Embed(
        title="Accuracy",
        description="__**â¤ÍŸÍŸÍÍŸÍŸÍŸÍŸÍÍŸÍŸÍŸÍÍŸÍŸÍŸÍŸÍÍŸÍŸÍÍŸÍŸÍŸÍŸÍÍŸÍŸÍŸÍÍŸÍŸÍŸÍŸÍŸÍŸÍŸÍŸÍÍŸÍŸÍŸÍÍŸÍŸÍŸÍŸÍÍŸÍŸÍÍŸÍŸÍŸÍŸÍÍŸÍŸÍŸÍÍŸÍŸÍŸÍŸÍŸÍŸÍŸÍŸÍÍŸÍŸÍŸÍÍŸÍŸÍŸÍŸÍÍŸÍŸÍÍŸÍŸÍŸÍŸÍÍŸÍŸÍŸÍÍŸÍŸÍŸÍŸÍŸÍŸÍŸÍŸÍÍŸÍŸÍŸÍÍŸÍŸÍŸÍŸÍÍŸÍŸÍÍŸÍŸÍŸÍŸÍÍŸÍŸÍŸÍÍŸÍŸÍŸÍŸÍŸÍŸÍŸÍŸÍÍŸÍŸÍŸÍÍŸÍŸÍŸÍŸÍÍŸÍŸÍÍŸÍŸÍŸÍŸÍÍŸÍŸÍŸÍâ¯ã€¢Game: {}\nâ¤ÍŸÍŸÍâ¯ã€¢Timing {}\nâ¤ÍŸÍŸÍâ¯ã€¢Accuracy: {}\nâ¤ÍŸÍŸÍâ¯ã€¢Winning Amount: {}\nâ¤ÍŸÍŸÍâ¯ã€¢Status: {}**__".format(gameName,time,accuracy,winAmt,status),
        colour=random.randint(0,0xfffff)
        )

    await ctx.send(embed=embed)
@acc.error
async def acc_error(ctx,error):
    if isinstance(error,commands.CheckFailure):
        await ctx.send("You need {} to use this command".format(gameRole))
    else:
        raise error


