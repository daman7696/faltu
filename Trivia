#🅃🅁🄸🅅🄸🄰 🄲🄾🄼🄼🄰🄽🄳🅂
@bot.command()
#@commands.has_role(accRole)
async def acc(ctx,gameName=None,time=None,accuracy=None,winAmt=None,*,status=None):
    embed=discord.Embed(
        title="Accuracy",
        description="__**⏤͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͟͞͞͞͞͞͞͞͞͞͞͞͞͞͞͞͞͞͞͞͞͞͞͞͞͞͞͞͞͞͞͞➯〢Game: {}\n⏤͟͟͞➯〢Timing {}\n⏤͟͟͞➯〢Accuracy: {}\n⏤͟͟͞➯〢Winning Amount: {}\n⏤͟͟͞➯〢Status: {}**__".format(gameName,time,accuracy,winAmt,status),
        colour=random.randint(0,0xfffff)
        )

    await ctx.send(embed=embed)
@acc.error
async def acc_error(ctx,error):
    if isinstance(error,commands.CheckFailure):
        await ctx.send("You need {} to use this command".format(gameRole))
    else:
        raise error


###EXAMPLE = {botPrefix}acc Loco 1:30PM 10/10 2.37rs All 🅆🄾🄽
